<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chenglei.humanrelationbooking">



    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:name=".BooksApp"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/BASE">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.chenglei.humanrelationbooking.home.HomeActivity"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".books.create.RecordCreateActivity"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar"
            android:screenOrientation="portrait"/>

        <activity android:name=".debug.FlowDebugActivity"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar"
            android:screenOrientation="portrait">

        </activity>

        <activity android:name=".debug.PagingDebugActivity"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar"
            android:screenOrientation="portrait"/>

        <activity android:name=".debug.DevelopActivity"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar"
            android:screenOrientation="portrait"/>

        <activity android:name=".base.ComposeActivity"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".h5.WebViewActivity"
            android:theme="@style/Theme.Humanrelationbooking.NoActionBar"
            android:screenOrientation="portrait"/>

    </application>

</manifest>